	</parameter>
	<queryString>
		<![CDATA[select hist.histcount,ln.linecount , req.ad_org_id as organization,req.m_requisition_id as reqid,usr.name as preparer,req.description,rl.total,cur.iso_code,
(select eut_convert_to_hijri(to_char(req.created,'yyyy-MM-dd'))) as creationdate, req.em_escm_doc_status,ref.name, (select eut_convert_to_hijri(to_char(now() ,'yyyy-MM-dd'))) as today,dept.name as deptname
 from m_requisition req
left join ad_user usr on usr.ad_user_id=req.createdby
left join ( select  sum(rl.linenetamt) as total,m_requisition_id from  m_requisitionline rl group by m_requisition_id)
rl on rl.m_requisition_id=req.m_requisition_id
left join (select ref.name,ref.value from ad_ref_list ref  where ref.ad_reference_id  ='131')  ref on ref.value=req.em_escm_doc_status
left join ( select count(escm_purchasereq_app_hist_id)as histcount,m_requisition_id from escm_purchasereq_app_hist group by  m_requisition_id )
hist on hist.m_requisition_id= req.m_requisition_id
left join ( select count(m_requisitionline_id)as linecount,m_requisition_id from m_requisitionline group by  m_requisition_id )
ln on ln.m_requisition_id= req.m_requisition_id
left join c_currency cur on cur.c_currency_id=req.c_currency_id
left join c_salesregion dept on dept.c_salesregion_id=req.em_escm_salesregion_id
where req.m_requisition_id   in $P!{REQUISITION_ID}]]>
	</queryString>
