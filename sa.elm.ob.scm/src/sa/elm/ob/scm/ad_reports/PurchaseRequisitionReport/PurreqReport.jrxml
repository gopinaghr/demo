	<subDataset name="Table Dataset 1" uuid="0978891a-75aa-470b-9750-3b768649be32">
		<parameter name="REQUISITION_ID" class="java.lang.String">
			<defaultValueExpression><![CDATA[""]]></defaultValueExpression>
		</parameter>
		<queryString>
			<![CDATA[select line,prd.value as itemcode,ln.description, uom.name as uomname,ln.suppliernotes ,ln.internalnotes,ln.qty,ln.priceactual,
(select eut_convert_to_hijri(to_char(ln.needbydate ,'yyyy-MM-dd'))) as needbydate,ln.linenetamt
 from m_requisitionline ln
left join m_product prd on prd.m_product_id =ln.m_product_id
left join c_uom uom on uom.c_uom_id=ln.c_uom_id
 where ln.m_requisition_id  = $P{REQUISITION_ID}
 order by line]]>
		</queryString>
		<field name="line" class="java.math.BigDecimal"/>
		<field name="itemcode" class="java.lang.String"/>
		<field name="description" class="java.lang.String"/>
		<field name="uomname" class="java.lang.String"/>
		<field name="suppliernotes" class="java.lang.String">
			<fieldDescription><![CDATA[--OBTG:NVARCHAR--]]></fieldDescription>
		</field>
		<field name="internalnotes" class="java.lang.String">
			<fieldDescription><![CDATA[--OBTG:NVARCHAR--]]></fieldDescription>
		</field>
		<field name="qty" class="java.math.BigDecimal"/>
		<field name="priceactual" class="java.math.BigDecimal"/>
		<field name="needbydate" class="java.lang.String">
			<fieldDescription><![CDATA[--OBTG:ONCREATEDEFAULT:SELECT DATEPLANNED FROM M_REQUISITIONLINE_ WHERE M_REQUISITIONLINE_.M_REQUISITIONLINE_ID = M_REQUISITIONLINE.M_REQUISITIONLINE_ID--]]></fieldDescription>
		</field>
		<field name="linenetamt" class="java.math.BigDecimal"/>
	</subDataset>
	<subDataset name="New Dataset 1" uuid="9fa6a936-613a-4900-967f-f72d0ed6fb70">
		<parameter name="REQUISITION_ID" class="java.lang.String">
			<defaultValueExpression><![CDATA[""]]></defaultValueExpression>
		</parameter>
		<queryString>
			<![CDATA[select hist.seqno ,(select  eut_convert_to_hijri(to_char( hist.approveddate ,'yyyy-MM-dd'))) as appdate,
usr.name  as preparer,hist.comments as notes,ref.name as action,hist.m_requisition_id
from escm_purchasereq_app_hist   hist
left join ad_user usr on usr.ad_user_id=hist.createdby
left join (select ref.name,ref.value from ad_ref_list ref  where ref.ad_reference_id  ='9F2DC8F55FE9442895FCD3ED468B1D50')
  ref on ref.value=hist.purchasereqaction
 where hist.m_requisition_id  = $P{REQUISITION_ID} order by seqno asc]]>
		</queryString>
		<field name="seqno" class="java.math.BigDecimal"/>
		<field name="appdate" class="java.lang.String"/>
		<field name="preparer" class="java.lang.String"/>
		<field name="notes" class="java.lang.String">
			<fieldDescription><![CDATA[--OBTG:NVARCHAR--]]></fieldDescription>
		</field>
		<field name="action" class="java.lang.String"/>
		<field name="m_requisition_id" class="java.lang.String"/>
	</subDataset>
	<parameter name="BASE_DESIGN" class="java.lang.String">
		<defaultValueExpression><![CDATA[""]]></defaultValueExpression>
	</parameter>
	<parameter name="SUBREPORT_DIR" class="java.lang.String" isForPrompting="false">
		<defaultValueExpression><![CDATA["/sa/elm/ob/scm/ad_reports/PurchaseRequisitionReport/"]]></defaultValueExpression>
	</parameter>
	<parameter name="REQUISITION_ID" class="java.lang.String">
