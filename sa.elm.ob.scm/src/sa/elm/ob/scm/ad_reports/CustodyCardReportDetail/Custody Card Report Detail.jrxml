    <parameter name="inpBeneficiaryType" class="java.lang.String"/>
    <parameter name="inpBeneficiaryId" class="java.lang.String"/>
    <parameter name="inpAD_Client_ID" class="java.lang.String"/>
    <parameter name="inpAD_Org_ID" class="java.lang.String"/>
    <parameter name="inpBeneficiaryName" class="java.lang.String"/>
    <parameter name="inpBeneficiaryCode" class="java.lang.String"/>
    <parameter name="inpBeneficiarytypes" class="java.lang.String"/>
    <parameter name="inpBeneficiaryNameCode" class="java.lang.String"/>
    <queryString>
        <![CDATA[select (row_number() over ()) as seq ,creationDate,org,client,clientid,productid,Itemdescription,itemcode,uom,MIRQty,RTQty,IRTQty,CTQty,LDQty,
documentno,tag,TransactionDate,type,Btype,Bname,bvalue,procurment,coalesce(currentqty,0) as currentqty
from ( select creationDate,org,client,clientid,
productid,Itemdescription,itemcode,uom,MIRQty,RTQty,IRTQty,CTQty,LDQty,documentno,tag,TransactionDate,type,Btype,Bname,bvalue,
(select procurement from escm_mrequest_custody  where    (documentno || coalesce(('-' || sn_no ),''))=tag and ad_client_id=clientid and ad_org_id=org and Beneficiary_Name=$P{inpBeneficiaryId} and

 Beneficiary_Type=$P{inpBeneficiaryType}) as procurment,currentqty from
((select req.ad_org_id as org,req.created as creationDate,cl.name as client,cl.ad_client_id as clientid,ln.m_product_id as productid,pro.name as Itemdescription,pro.value as itemcode,coalesce(uomtrl.name,uom.name) as uom,
case when wh.em_escm_warehouse_type='MAW' then 1::numeric else 0 end as MIRQty,0 as RTQty,
case when wh.em_escm_warehouse_type='RTW' then 1 ::numeric else 0 end as IRTQty,
0 as CTQty,0 as LDQty,req.documentno,(cus.documentno || coalesce(('-' || cus.sn_no ),'')) as tag,
eut_cnvrttohjr_yyyymmdd(to_char(req.Transaction_Date,'YYYY-MM-DD')) as TransactionDate,'MIR' as type,
coalesce(trl.name,list.name) as Btype,substring(ben.name,length(ben.value ) + 2) as Bname,ben.value as bvalue,onhandqty.openQty as currentqty
from  escm_material_request req
join  escm_material_reqln ln on req.escm_material_request_id=ln.escm_material_request_id
join  escm_mrequest_custody cus on cus.escm_material_reqln_id=ln.escm_material_reqln_id
join m_product pro on ln.m_product_id=pro.m_product_id
left join (select sum(Qty) as openQty,m_product_id from escm_mrequest_custody where Beneficiary_Type=$P{inpBeneficiaryType} and Beneficiary_Name=$P{inpBeneficiaryId}
 and Status='IU' group by m_product_id) onhandqty
 on onhandqty.m_product_id=pro.m_product_id
join c_uom uom on pro.c_uom_id=uom.c_uom_id
left join c_uom_trl uomtrl on uomtrl.c_uom_id=uom.c_uom_id and uomtrl.ad_language  ='ar_SA'
join ad_client cl on cl.ad_client_id=req.ad_client_id
left join Escm_Beneficiary_V ben on ben.Escm_Beneficiary_V_id=req.Beneficiary_Name
left join ad_ref_list list on list.value=req.Beneficiary_Type and list.ad_reference_id='E585F9EEA3024736B3E30F9F6A7C9A09'
left join AD_Ref_List_Trl trl on list.ad_ref_list_id=trl.ad_ref_list_id and trl.ad_language='ar_SA'
left join m_warehouse wh on req.m_warehouse_id=wh.m_warehouse_id
where req.Beneficiary_Name=$P{inpBeneficiaryId}
and req.Beneficiary_Type=$P{inpBeneficiaryType} and cus.status != 'N'
group by cus.documentno,cus.sn_no,req.documentno,delivered_qty,req.created,pro.name,cl.ad_client_id,ln.m_product_id,uomtrl.name,uom.name,pro.value,req.Transaction_Date,cl.name,req.ad_org_id,list.name,ben.name,ben.value,onhandqty.openQty,trl.name,wh.em_escm_warehouse_type order by req.documentno)
union all(
    select req.ad_org_id as org,req.created as creationDate,cl.name as client,cl.ad_client_id as clientid,ln.m_product_id as productid,pro.name as Itemdescription,pro.value as itemcode,coalesce(uomtrl.name,uom.name) as uom,
case when wh.em_escm_warehouse_type='MAW' then 1::numeric else 0 end as MIRQty,0 as RTQty,
case when wh.em_escm_warehouse_type='RTW' then 1 ::numeric else 0 end as IRTQty,
0 as CTQty,0 as LDQty,req.documentno,(cus.documentno || coalesce(('-' || cus.sn_no ),'')) as tag,
eut_cnvrttohjr_yyyymmdd(to_char(req.Transaction_Date,'YYYY-MM-DD')) as TransactionDate,'MIR' as type,
coalesce(trl.name,list.name) as Btype,substring(ben.name,length(ben.value ) + 2) as Bname,ben.value as bvalue,onhandqty.openQty as currentqty
from  escm_material_request req
join  escm_material_reqln ln on req.escm_material_request_id=ln.escm_material_request_id
join  escm_custody_transaction trans on trans.escm_material_reqln_id=ln.escm_material_reqln_id and trans.btype=$P{inpBeneficiaryType} and trans.bname=$P{inpBeneficiaryId}
join  escm_mrequest_custody cus on cus.escm_mrequest_custody_id=trans.escm_mrequest_custody_id and cus.beneficiary_type=$P{inpBeneficiaryType} and cus.beneficiary_name=$P{inpBeneficiaryId}
join m_product pro on ln.m_product_id=pro.m_product_id
left join (select sum(Qty) as openQty,m_product_id from escm_mrequest_custody where Beneficiary_Type=$P{inpBeneficiaryType} and Beneficiary_Name=$P{inpBeneficiaryId}
 and Status='IU' group by m_product_id) onhandqty
 on onhandqty.m_product_id=pro.m_product_id
join c_uom uom on pro.c_uom_id=uom.c_uom_id
left join c_uom_trl uomtrl on uomtrl.c_uom_id=uom.c_uom_id and uomtrl.ad_language  ='ar_SA'
join ad_client cl on cl.ad_client_id=req.ad_client_id
left join Escm_Beneficiary_V ben on ben.Escm_Beneficiary_V_id=req.Beneficiary_Name
left join ad_ref_list list on list.value=req.Beneficiary_Type and list.ad_reference_id='E585F9EEA3024736B3E30F9F6A7C9A09'
left join AD_Ref_List_Trl trl on list.ad_ref_list_id=trl.ad_ref_list_id and trl.ad_language='ar_SA'
left join m_warehouse wh on req.m_warehouse_id=wh.m_warehouse_id
where req.Beneficiary_Name=$P{inpBeneficiaryId}
and req.Beneficiary_Type=$P{inpBeneficiaryType} and cus.status != 'N'
group by cus.documentno,cus.sn_no,req.documentno,delivered_qty,req.created,pro.name,cl.ad_client_id,ln.m_product_id,uomtrl.name,uom.name,pro.value,req.Transaction_Date,cl.name,req.ad_org_id,list.name,ben.name,ben.value,onhandqty.openQty,trl.name,wh.em_escm_warehouse_type order by req.documentno)
union all
(select rettran.ad_org_id,rettran.created as creationDate,cl.name as client,cl.ad_client_id as clientid,ln.m_product_id as productid,pro.name as Itemdescription,pro.value as itemcode,coalesce(uomtrl.name,uom.name) as uom,
0 as MIRQty,case when rettran.EM_Escm_Receivingtype='INR' then (1 * -1) ::numeric else 0 end as RTQty, 0 as IRTQty,0 as CTQty,
case when rettran.EM_Escm_Receivingtype='LD' then (1 * -1) ::numeric else 0 end as LDQty,
rettran.documentno,(cus.documentno || coalesce(('-' || cus.sn_no ),'')) as tag,
eut_cnvrttohjr_yyyymmdd(to_char(rettran.MovementDate,'YYYY-MM-DD')) as TransactionDate,rettran.EM_Escm_Receivingtype as type,
coalesce(trl.name,list.name) as Btype,substring(ben.name,length(ben.value ) + 2) as Bname,ben.value as bvalue,onhandqty.openQty as currentqty
from  M_InOut rettran
join M_InOutLine ln on rettran.m_inout_id=ln.m_inout_id
join m_product pro on ln.m_product_id=pro.m_product_id
left join (select sum(Qty) as openQty,m_product_id from escm_mrequest_custody where Beneficiary_Type=$P{inpBeneficiaryType} and Beneficiary_Name=$P{inpBeneficiaryId}
 and Status='IU' group by m_product_id) onhandqty
 on onhandqty.m_product_id=pro.m_product_id
join c_uom uom on pro.c_uom_id=uom.c_uom_id
left join c_uom_trl uomtrl on uomtrl.c_uom_id=uom.c_uom_id and uomtrl.ad_language  ='ar_SA'
join Escm_custody_transaction custrans on ln.m_inoutline_id=custrans.m_inoutline_id
join escm_mrequest_custody cus on custrans.escm_mrequest_custody_id=cus.escm_mrequest_custody_id
join ad_client cl on cl.ad_client_id=rettran.ad_client_id
left join Escm_Beneficiary_V ben on ben.Escm_Beneficiary_V_id=rettran.EM_Escm_Bname
left join ad_ref_list list on list.value=rettran.EM_Escm_Btype and list.ad_reference_id='E585F9EEA3024736B3E30F9F6A7C9A09'
left join AD_Ref_List_Trl trl on list.ad_ref_list_id=trl.ad_ref_list_id and trl.ad_language='ar_SA'
where EM_Escm_Receivingtype in('INR','IRT','IR','LD') and em_escm_iscustody_transfer <>'Y'and rettran.EM_Escm_Bname=$P{inpBeneficiaryId}
and rettran.EM_Escm_Btype=$P{inpBeneficiaryType} and custrans.isprocessed='Y'
group by cus.documentno,cus.sn_no,rettran.documentno,rettran.created,ln.MovementQty,cl.ad_client_id,pro.name,ln.m_product_id,uomtrl.name,uom.name,pro.value,rettran.MovementDate,rettran.EM_Escm_Receivingtype,cl.name,rettran.ad_org_id,list.name,
 ben.name,ben.value,onhandqty.openQty,trl.name order by rettran.documentno)
union all
(select rettran.ad_org_id,rettran.created as creationDate,cl.name as client,cl.ad_client_id as clientid,ln.m_product_id as productid,pro.name as Itemdescription,pro.value as itemcode,coalesce(uomtrl.name,uom.name) as uom,0 as MIRQty, 0 as RTQty,
 0 as IRTQty,case when  rettran.EM_Escm_Bname=$P{inpBeneficiaryId} and rettran.EM_Escm_Btype=$P{inpBeneficiaryType} then (1 * -1)::numeric when
 rettran.EM_Escm_Tobenefi_Name=$P{inpBeneficiaryId} and rettran.EM_Escm_Tobeneficiary=$P{inpBeneficiaryType} then 1::numeric else 0 end as CTQty,
 0 as LDQty,
rettran.documentno,(cus.documentno || coalesce(('-' || cus.sn_no ),'')) as tag,
eut_cnvrttohjr_yyyymmdd(to_char(rettran.MovementDate,'YYYY-MM-DD')) as TransactionDate,rettran.EM_Escm_Receivingtype as type,
coalesce(trl.name,list.name) as Btype,substring(ben.name,length(ben.value ) + 2) as Bname,ben.value as bvalue,onhandqty.openQty as currentqty
from  M_InOut rettran
join M_InOutLine ln on rettran.m_inout_id=ln.m_inout_id
join m_product pro on ln.m_product_id=pro.m_product_id
left join (select sum(Qty) as openQty,m_product_id from escm_mrequest_custody where Beneficiary_Type=$P{inpBeneficiaryType} and Beneficiary_Name=$P{inpBeneficiaryId}
 and Status ='IU' group by m_product_id) onhandqty
 on onhandqty.m_product_id=pro.m_product_id
join c_uom uom on pro.c_uom_id=uom.c_uom_id
left join c_uom_trl uomtrl on uomtrl.c_uom_id=uom.c_uom_id and uomtrl.ad_language  ='ar_SA'
join Escm_custody_transaction custrans on ln.m_inoutline_id=custrans.m_inoutline_id
join escm_mrequest_custody cus on custrans.escm_mrequest_custody_id=cus.escm_mrequest_custody_id
join ad_client cl on cl.ad_client_id=rettran.ad_client_id
left join Escm_Beneficiary_V ben on ben.Escm_Beneficiary_V_id=rettran.EM_Escm_Bname
left join ad_ref_list list on list.value=rettran.EM_Escm_Btype and list.ad_reference_id='E585F9EEA3024736B3E30F9F6A7C9A09'
left join AD_Ref_List_Trl trl on list.ad_ref_list_id=trl.ad_ref_list_id and trl.ad_language='ar_SA'
where EM_Escm_Receivingtype in('IR') and em_escm_iscustody_transfer ='Y'
and(
rettran.EM_Escm_Bname=$P{inpBeneficiaryId} or  rettran.EM_Escm_Tobenefi_Name=$P{inpBeneficiaryId} )
and (rettran.EM_Escm_Btype=$P{inpBeneficiaryType}  or  rettran.EM_Escm_Tobeneficiary=$P{inpBeneficiaryType} )
and custrans.isprocessed='Y'
group by cus.documentno,cus.sn_no,rettran.documentno,rettran.created,ln.MovementQty,pro.name,cl.ad_client_id,uomtrl.name,uom.name,ln.m_product_id,pro.value,rettran.EM_Escm_Tobeneficiary, rettran.EM_Escm_Tobenefi_Name,rettran.EM_Escm_Btype,rettran.MovementDate,rettran.EM_Escm_Receivingtype,rettran.EM_Escm_Bname,cl.name,rettran.ad_org_id,list.name,
 ben.name,ben.value,onhandqty.openQty,trl.name order by rettran.documentno)
union (
 select det.ad_org_id,det.created as creationDate ,cl.name as client,cl.ad_client_id as clientid,
 pro.m_product_id as productid,pro.name as Itemdescription,pro.value as itemcode,coalesce(uomtrl.name,uom.name) as uom,
 1::numeric as MIRQty,0 as RTQty,0 as IRTQty,
0 as CTQty,0 as LDQty,tran.documentno,(det.documentno || coalesce(('-' || det.sn_no ),'')) as tag,
eut_cnvrttohjr_yyyymmdd(to_char(tran.transaction_date,'YYYY-MM-DD')) as TransactionDate,'MIR' as type,
coalesce(trl.name,list.name) as Btype,substring(ben.name,length(ben.value ) + 2) as Bname,ben.value as bvalue,
onhandqty.openQty as currentqty
  from escm_mrequest_custody det
    join escm_custody_transaction tran on tran.escm_mrequest_custody_id=det.escm_mrequest_custody_id
  join ad_client cl on cl.ad_client_id=det.ad_client_id
join m_product pro on det.m_product_id=pro.m_product_id
join c_uom uom on pro.c_uom_id=uom.c_uom_id
left join c_uom_trl uomtrl on uomtrl.c_uom_id=uom.c_uom_id and uomtrl.ad_language  ='ar_SA'
left join (select sum(Qty) as openQty,m_product_id from escm_mrequest_custody where Beneficiary_Type=$P{inpBeneficiaryType}
and Beneficiary_Name=$P{inpBeneficiaryId}
 and Status='IU' group by m_product_id) onhandqty   on onhandqty.m_product_id=pro.m_product_id

left join Escm_Beneficiary_V ben on ben.Escm_Beneficiary_V_id=det.Beneficiary_Name
left join ad_ref_list list on list.value=det.Beneficiary_Type and list.ad_reference_id='E585F9EEA3024736B3E30F9F6A7C9A09'
left join AD_Ref_List_Trl trl on list.ad_ref_list_id=trl.ad_ref_list_id and trl.ad_language='ar_SA'
where det.Beneficiary_Name=$P{inpBeneficiaryId} and
 det.Beneficiary_Type=$P{inpBeneficiaryType} and tran.m_inoutline_id is null and det.escm_material_reqln_id is null
group by det.documentno,det.sn_no,tran.documentno,det.created,pro.name,
cl.ad_client_id,pro.m_product_id,uomtrl.name,uom.name,pro.value,tran.Transaction_Date,cl.name,
det.ad_org_id,list.name,ben.name,ben.value,onhandqty.openQty,trl.name order by tran.documentno )) as a
order by Itemdescription,TransactionDate desc,creationDate desc) b]]>
    </queryString>
